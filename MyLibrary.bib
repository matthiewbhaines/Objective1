	issn = {15280012},
	url = {https://doi.org/10.1053/j.gastro.2021.05.047},
	doi = {10.1053/j.gastro.2021.05.047},
	abstract = {Background \& Aims: This study compared the effectiveness of the Specific Carbohydrate Diet ({SCD}) to the Mediterranean diet ({MD}) as treatment for Crohn's disease ({CD}) with mild to moderate symptoms. Methods: Adult patients with {CD} and with mild-to-moderate symptoms were randomly assigned 1:1 to consume the {MD} or {SCD} for 12 weeks. For the first 6 weeks, participants received prepared meals and snacks according to their assigned diet. After 6 weeks, participants were instructed to follow the diet independently. The primary outcome was symptomatic remission at week 6. Key secondary outcomes at week 6 included fecal calprotectin ({FC}) response ({FC} {\textless}250 μg/g and reduction by {\textgreater}50\% among those with baseline {FC} {\textgreater}250 μg/g) and C-reactive protein ({CRP}) response (high-sensitivity {CRP} {\textless}5 mg/L and {\textgreater}50\% reduction from baseline among those with high-sensitivity {CRP} {\textgreater}5 mg/L). Results: The study randomized 194 patients, and 191 were included in the efficacy analyses. The percentage of participants who achieved symptomatic remission at week 6 was not superior with the {SCD} ({SCD}, 46.5\%; {MD}, 43.5\%; P = .77). {FC} response was achieved in 8 of 23 participants (34.8\%) with the {SCD} and in 4 of 13 participants (30.8\%) with the {MD} (P = .83). {CRP} response was achieved in 2 of 37 participants (5.4\%) with the {SCD} and in 1 of 28 participants (3.6\%) with the {MD} (P = .68). Conclusions: The {SCD} was not superior to the {MD} to achieve symptomatic remission, {FC} response, and {CRP} response. {CRP} response was uncommon. Given these results, the greater ease of following the {MD} and other health benefits associated with the {MD}, the {MD} may be preferred to the {SCD} for most patients with {CD} with mild to moderate symptoms. {ClinicalTrials}.gov Identifier: {NCT}03058679},
	pages = {837--852.e9},
	number = {3},
	journaltitle = {Gastroenterology},
	author = {Lewis, James D. and Sandler, Robert S. and Brotherton, Carol and Brensinger, Colleen and Li, Hongzhe and Kappelman, Michael D. and Daniel, Scott G. and Bittinger, Kyle and Albenberg, Lindsey and Valentine, John F. and Hanson, John S. and Suskind, David L. and Meyer, Andrea and Compher, Charlene W. and Bewtra, Meenakshi and Saxena, Akriti and Dobes, Angela and Cohen, Benjamin L. and Flynn, Ann D. and Fischer, Monika and Saha, Sumona and Swaminath, Arun and Yacyshyn, Bruce and Scherl, Ellen and Horst, Sara and Curtis, Jeffrey R. and Braly, Kimberly and Nessel, Lisa and {McCauley}, Maureen and {McKeever}, Liam and Herfarth, Hans},
	date = {2021},
	pmid = {34052278},
	note = {Publisher: Elsevier, Inc},
	keywords = {Clinical Trial, Comparative Effectiveness, Mediterranean Diet, Nutrition, Randomization},
	file = {1-s2.0-S0016508521030699-main:C\:\\Users\\Matthiew\\Documents\\Research\\Mendeley\\1-s2.0-S0016508521030699-main.pdf:application/pdf},
}

@incollection{tresserra-rimbauCoffeePolyphenolsHigh2015,
	title = {Coffee Polyphenols and High Cardiovascular Risk Parameters},
	isbn = {978-0-12-416716-2},
	url = {http://dx.doi.org/10.1016/B978-0-12-409517-5.00042-5},
	abstract = {Polyphenols are known to reduce the risk of cardiovascular diseases, which constitute an enormous health problem with a high socioeconomic impact worldwide. The main source of phenolic acids and hydroxycinnamic acids in Western diets is coffee. Considerable research has been dedicated to the effects of coffee as a beverage but only few studies, mainly in vitro, cell, and animal studies, have focused on the specific effects of its main phenolic compound, chlorogenic acid (43-117. mg/100. ml). Some of these studies have found that chlorogenic acid has potential cardiovascular benefits related to the lowering of blood pressure, endothelial function improvement, low-density lipoprotein cholesterol oxidation, and nitric oxide bioavailability. However, further work is needed to investigate the long-term effects of human ingestion of chlorogenic acid from coffee, especially in subjects with cardiovascular diseases.},
	pages = {387--394},
	booktitle = {Coffee in Health and Disease Prevention},
	publisher = {Elsevier Inc.},
