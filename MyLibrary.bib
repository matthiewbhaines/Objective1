@misc{leeWhatTemperaturesConsumers2002,
	title = {At What Temperatures Do Consumers Like to Drink Coffee?: Mixing Methods},
	abstract = {Three hundred consumers were required to mix a hot and a cooler coffee together until it was at a desired temperature for drinking. They added creamer and sweetener to taste. In a 2nd experiment, 108 consumers performed the same experiment with black coffee only, but repeated it using different coffee strengths. In all experiments, the chosen mean preferred temperature for drinking was around 60 °C (140 °F). Black coffee drinkers chose a slightly higher mean temperature than drinkers with added creamer, and they also chose a slightly lower mean temperature when the flavor was stronger. In all cases, consumers tended to choose, on average, temperatures for drinking coffee that were above the oral pain threshold and the burn damage threshold.},
