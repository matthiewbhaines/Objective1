	file = {Full Text PDF:C\:\\Users\\Matthiew\\Zotero\\storage\\SAHV9679\\Djordjevic et al. - 2004 - Effects of Perceived and Imagined Odors on Taste Detection.pdf:application/pdf;Snapshot:C\:\\Users\\Matthiew\\Zotero\\storage\\TQL7CIIK\\321312.html:text/html},
