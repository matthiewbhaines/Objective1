	file = {PDF:C\:\\Users\\Matthiew\\Zotero\\storage\\QDRAD3FZ\\Batali et al. - 2021 - Titratable Acidity, Perceived Sourness, and Liking of Acidity in Drip Brewed Coffee.pdf:application/pdf},
