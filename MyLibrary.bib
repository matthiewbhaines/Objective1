	langid = {english},
	keywords = {color, bitter, sensory, aroma, consumer, grapefruit, hedonic, naringin, sweet},
	file = {Full Text PDF:C\:\\Users\\Matthiew\\Zotero\\storage\\YI4CR76B\\Gous et al. - 2019 - Effects of Varying the Color, Aroma, Bitter, and Sweet Levels of a Grapefruit-Like Model Beverage on.pdf:application/pdf;nutrients-11-00464-v2 (1):C\:\\Users\\Matthiew\\Zotero\\storage\\73TDQ2ZG\\nutrients-11-00464-v2 (1).pdf:application/pdf;PDF:C\:\\Users\\Matthiew\\Zotero\\storage\\WJLAFPCM\\nutrients-11-00464-v2.pdf:application/pdf},
