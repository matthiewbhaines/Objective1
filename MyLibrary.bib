Issue: 2
Publication Title: Foods
Volume: 10},
	keywords = {Consumer perception, Fish alternatives, Flexitarian, Fortified food, Meat alternatives, Nutrients, Plant-based dairy, Plant-based diet, Vegan, Vegetarian},
}

