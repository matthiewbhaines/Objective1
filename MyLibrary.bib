@article{prescottInhibitionEvaluativePerceptual2009,
	title = {Inhibition of evaluative and perceptual odour-taste learning by attention to the stimulus elements},
	volume = {62},
	issn = {17470218},
	doi = {10.1080/17470210903031169},
	abstract = {Pairing of odours and tastes in solution results in the odour taking on the taste's hedonic and perceptual properties. Theoretical accounts of such conditioning propose that the stimulus elements form a configuration with hedonic and/or perceptual properties. One implication of this account is that configural binding may be inhibited by allocation of attention to the individual elements during conditioning (analytical attention). This was tested here by training participants to attend analytically or synthetically during pairing of odours with either sucrose or water. Differential conditioning effects for sucrose- and water-paired odours were seen only in the synthetic group, with sucrose-paired odours increasing in both smelled sweetness and liking. These data support configural models as explanations for these forms of associative learning and emphasize the role of top-down processes in mediating transfer of perceptual and hedonic properties from tastes to odours. © 2009 The Experimental Psychology Society.},
	pages = {2133--2140},
	number = {11},
	journaltitle = {Quarterly Journal of Experimental Psychology},
	author = {Prescott, John and Murphy, Samantha},
	date = {2009-04},
	pmid = {19565428},
	keywords = {Taste, Attention, Evaluative conditioning, Odour, Perceptual conditioning},
}

@misc{GENERALEQUIDISTANCESCALE,
	title = {A {GENERAL} {EQUI}-{DISTANCE} {SCALE} {OF} {THE} {FOUR} {QUALITIES} {OF} {TASTE}},
	abstract = {An equi-distance scale of intensity for the four primary qualities of taste has been constructed. Direct assessments of difference in intensity were made of two solutions presented in pairs; pairs of solutions of different concentration of the same substance or pairs of solutions of different substances.With a special procedure of analysis, it has been shown that a single common core component is definable throughout eight Ss' individual criteria for the comparative judgments and the scale has been defined on the core component. The Ss consisted of 4 specialists of taste-testing and 4 laymen. Intensity of taste on this scale is logarithmic functions of concentration with the slopes almost the same for the four qualities. The present scale based on difference judgments is related to the gust-scale based on ratio judgments in the manner as expected from the general trend in prothetic continua.},
}

@article{stevensonConfusingTastesSmells1999,
	title = {Confusing Tastes and Smells: How Odours can Influence the Perception of Sweet and Sour Tastes},
	volume = {24},
	url = {https://academic.oup.com/chemse/article/24/6/627/320328},
	doi = {https://doi.org/10.1093/chemse/24.6.627},
	abstract = {This study investigated the relationship between perception of an odour when smelled and the taste of a solution to which the odour is added as a flavorant. In Experiment 1 (E1) sweetness, sourness, liking and intensity ratings were obtained for 20 odours. Taste ratings were then obtained for sucrose solutions to which the odours had been added as flavorants. Certain odours were found to enhance tasted sweetness while others suppressed it. The degree to which an odour smelled sweet was the best predictor of the taste ratings. These findings were extended in Experiment 2 (E2), which included a second tastant, citric acid, and employed four odours from E1. The most sweet smelling odour, caramel, was found to suppress the sourness of citric acid and, as in E1, to enhance the sweetness of sucrose. Again, odours with low sweetness suppressed the sweetness of tasted sucrose. The study demonstrated that the effects of odours on taste perception are not limited to sweetness enhancement and apply to sour as well as sweet tastes. The overall pattern of results is consistent with an explanation of the taste properties of odours in terms of prior flavour-taste associations.},
	pages = {627--635},
	number = {6},
	journaltitle = {Chemical Senses},
	author = {Stevenson, Richard J. and Prescott, John and Boakes, Robert A.},
	date = {1999},
	file = {PDF:C\:\\Users\\Matthiew\\Zotero\\storage\\HTYKVITG\\Stevenson - 1999 - Confusing Tastes and Smells How Odours can Influence the Perception of Sweet and Sour Tastes.pdf:application/pdf},
}

