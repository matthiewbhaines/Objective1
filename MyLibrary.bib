@article{stevensonChangesOdorSweetness1998,
	title = {Changes in Odor Sweetness Resulting from Implicit Learning of a Simultaneous Odor-Sweetness Association: An Example of Learned Synesthesia},
	volume = {29},
	issn = {0023-9690},
	doi = {10.1006/LMOT.1998.0996},
	abstract = {In two experiments the smelled sweetness of odors was increased by using them as flavorants of sucrose solution. Experiment 1 used blind experimenters to compare a target odor mixed with sucrose with a control odor mixed with water during masked training trials. The increased sweetness of the target odor was unaffected by whether or not subjects revealed some explicit knowledge of the contingencies in a post-conditioning recognition test. Experiment 2 found that such a conditioned increase in odor sweetness occurred whether training solutions were sipped from a cup or sucked through a straw. Using a frequency test designed to provide a sensitive assay of contingency awareness, there was still no indication that this affected conditioning. It was concluded that such modification of the taste-properties of odors results from implicit simultaneous associative learning and provides an example of learned synesthesia. © 1998 Academic Press.},
	pages = {113--132},
	number = {2},
	journaltitle = {Learning and Motivation},
	author = {Stevenson, Richard J. and Boakes, Robert A. and Prescott, John},
	date = {1998-05},
	note = {Publisher: Academic Press},
	keywords = {Taste, Awareness, Implicit learning, Odor, Simultaneous conditioning, Synesthesia},
}

@article{iannilliOralTextureInfluences2014,
	title = {Oral texture influences the neural processing of ortho- and retronasal odors in humans},
	volume = {1587},
	issn = {0006-8993},
	doi = {10.1016/J.BRAINRES.2014.08.063},
	abstract = {Eating implies mutual interactions between different senses. In the present work we aimed at studying relations between food texture and food odor, using both psychophysical and imaging techniques. Eighteen right-handed healthy human subjects participated to both behavioral and {fMRI} sessions. Fresh, sweetened milk and a more thickened version were delivered orally; in addition, a buttery-cream aroma was presented ortho- or retronasally. Stimuli were applied using a gustometer and or an air-dilution olfactometer, both computer-controlled. In each session subjects rated separately odor-, taste- and thickness intensities of the stimuli. The behavioral data show that odors, presented through either retro- or orthonasal path, induce a significant flavor enhancement with respect to the no-odor condition. Brain functional data indicated a significant enhancement of the activation of olfactory eloquent areas in favor of ortho-nasal odor presentation while activations of mechanosensory areas were favored by the retro-nasal odor route. As effect of oral stimuli we found a significant correlation between the texture intensity rating vs. the {BOLD} signal in the supplementary motor area, known to drive subconsciously primed movement, putatively associated in this case with the tongue movement required with the handling of the stimulus. Moreover, we found inhibition of the signal in different sensory specific areas as an effect of the mutual interaction between stimulus qualities. In conclusion, ortho- and retronasal odors differentially affect the neural processing of the texture of oral stimuli.},
	pages = {77--87},
	number = {1},
	journaltitle = {Brain Research},
	author = {Iannilli, Emilia and Bult, Johannes H. F. and Roudnitzky, Natacha and Gerber, Johannes and Wijk, Rene A. De and Hummel, Thomas},
	date = {2014-10},
	pmid = {25175838},
	note = {Publisher: Elsevier},
	keywords = {Taste, Flavor, {FMRI}, Orthonasal, Retronasal, Texture},
}

@article{murphyTasteOlfactionIndependence1980,
	title = {Taste and Olfaction: Independence vs Interaction},
	volume = {24},
	abstract = {{MURPHY}, C. {AND} W. S. {CAIN}. Taste and olfaction: Independence vs interaction. {PHYSIOL}. {BEHAV}. 24(3) 601-605, 1980.-Twenty persons sipped and judged overall perceived magnitude, odor magnitude, and taste magnitude of various concentrations of the odorant citral, the tastants sodium chloride and sucrose, and odorant-tastant combinations. In a second experiment, the same twenty persons sniffed and judged perceived odor magnitude for the same set of stimuli. The investigation probed two primary questions: Does the apparent harmony of an olfactory-taste mixture dictate the degree of additivity in that mixture? Does harmony of the components influence the production of any taste-smell confusion? The data from both the harmonious mixture (citral and sucrose) and the dissonant mixture (citral and {NaCI}) imply absence of sensory inhibition or facilitation between taste and olfaction. Nevertheless, both types of mixtures led to substantial taste-{smeU} confusion whereby olfactory stimulation evoked sensations of taste. Taste Olfaction Additivity {PREVIOUS} research demonstrated that when stimulated with an odor-taste mixture the olfactory and taste systems behaved in an approximately additive manner, that is, approximately independently [6]. Despite this apparent independence of functioning, persons often confused the locus of stimulation. They perceived olfactory stimulation as taste, a mislocalization long known but rarely explored [3]. The present investigation probes the phenomena of additivity and mislocalization further. The primary question concerns whether the harmony of an odor-taste mixture determines the degree of additivity. Citral (an odorant with both a sweet and a lemon smell) and sucrose served here as the harmonious mixture, and citral and sodium chloride as the dissonant mixture. In our culture, fruit beverages flavored with sucrose are familiar, congruous, common. Fruit beverages flavored with sodium chloride are unfamiliar, incongruous, the exception. Sucrose and {NaCl} were chosen both for their relationships to citral and for their prototypical appeal only to taste and not to olfaction. {METHOD} Experiment 1 Subjects. Fourteen women and six men served. All but one had previous experience in psychophysical experiments. Stimuli. The stimuli were three concentrations each of citral, sucrose, sodium chloride, all nine possible mixtures of citral and sucrose, all nine possible mixtures of citral and sodium chloride, and deionized water alone. There were therefore 28 stimuli. Citral, a lipid soluble odorant, was first dissolved in ethyl alcohol in the proportions 1 to 20. The resulting solution, soluble in water, permitted preparation of aqueous concentrations of 0.001, 0.003, and 0.006\% citral. The accompanying small amounts of the relatively ineffective odorant ethyl alcohol (0.019, 0.057, and 0.114\% respectively) present only to facilitate the water solubility of citral, were well below thresholds for taste or for trigeminal sensation and produced no discernible sensory impact [5, 10, 11]. Concentrations of sucrose and {NaCl} were 0.04, 0.12, and 0.36 M. The stimuli were presented at room temperature in disposable polyethylene cups. Gas-chromatography (flame ionization detector) revealed a proportionality between nominal and vapor-phase concentration of citral over the range employed, 0.001\% to 0.006\% (see [1] for a description of the vessels used to assess vapor-phase concentration). The addition of sucrose or sodium chloride to the solutions of citral caused no systematic change in vapor-phase concentration. Hence, the odorant neither gained nor lost potency when mixed with the in-volatile taste stimuli. Procedure. Each subject participated in six 1.0-1.5 hr sessions. The subjects assessed perceived intensity by means of magnitude estimation with no prescribed modulus [8]. That is, they assigned numbers proportional to perceived intensity with no constraints on the number chosen first. Upon presentation of each sample, the subject rinsed with deionized water, expectorated, sipped approximately 10 ml of the stimulus, expectorated, and recorded a response.},
