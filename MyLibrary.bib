	journaltitle = {Physiology \& Behavior},
	author = {Murphy, Claire and Cain, William S.},
	date = {1980},
	note = {Publisher: Pergamon Press and Brain Research Publ},
	file = {1980MurphyCain:C\:\\Users\\Matthiew\\Documents\\Research\\Mendeley\\1980MurphyCain.pdf:application/pdf},
}

