@article{frankStrawberryOdorNot1989,
	title = {Strawberry odor, but not red color, enhances the sweetness of sucrose solutions},
	volume = {14},
	url = {https://academic.oup.com/chemse/article/14/3/371/470587},
	abstract = {Several investigators have recently reported that odor and color can influence the sweetness of stimuli containing sucrose. These effects were examined further by assessing the effect of red coloring and strawberry odorant on the sweetness ratings of aqueous solutions of sucrose. Two methods of olfactory stimulation were used: (i) sip and spit, and (ii) swallowing the solutions. It was anticipated that swallowing the solutions would enhance the olfactory effects by augmenting retronasal olfactory stimulation. The strawberry odor induced a small, but significant increase in sweetness for both the sip and spit and swallowing condition. There was a tendency for this effect to be more consistent and stable in the swallowing condition. Red coloring had no effect on sweetness.},
	pages = {371--377},
	number = {3},
	journaltitle = {Chemical Senses},
