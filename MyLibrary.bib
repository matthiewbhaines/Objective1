@article{bernsteinLearnedTasteAversions1980,
	title = {Learned taste aversions in humans},
	volume = {25},
	issn = {00319384},
	doi = {10.1016/0031-9384(80)90274-7},
	abstract = {Taste aversion learning was examined in human adults. Adult patients receiving chemotherapeutic drugs were exposed to one of two distinctively flavored ice creams in a single conditioning session. Flavor preference were subsequently evaluated in a two-ice cream choice test. Significant reductions in preference for the specific flavor consumed before drug treatments were seen. Therefore, adults, like children (and numerous subhuman species) acquire learned taste aversions after a single pairing of a conditioned stimulus ({CS}) (taste) and an unconditioned stimulus ({US}) (drug treatment). Results are discussed in terms of their theoretical as well as practical implications. © 1980, All rights reserved.},
