@incollection{shibamotoVolatileChemicalsThermal2015,
	title = {Volatile Chemicals from Thermal Degradation of Less Volatile Coffee Components},
	isbn = {978-0-12-416716-2},
	abstract = {There have been more than 800 volatile chemicals reported in roasted coffee to date. The major volatile chemicals that contribute to roasted coffee flavors are heterocyclic compounds. Most of these heterocyclic compounds form from the Maillard reaction occurring between less volatile coffee components, the sugars/carbohydrates/lipids and amino acids/proteins in green coffee beans. However, in addition to the Maillard reaction products, many volatile chemicals forming from the degradation of these less volatile chemicals have been found in roasted coffee beans. These volatile chemicals are low molecular weight carbonyl compounds, such as acetaldehyde, glyoxal, methyl glyoxal, and diacetyl. Also, many volatile chemicals, in particular furans and aromatic compounds, are found in heated chlorogenic acids, one of the major components of green coffee beans. Furans and aromatic compounds are proposed to form from a quinic acid moiety and a caffeic acid moiety of a chlorogenic acid, respectively, via the degradation reactions.},
	pages = {129--135},
	booktitle = {Coffee in Health and Disease Prevention},
	publisher = {Elsevier Inc.},
	author = {Shibamoto, Takayuki},
	date = {2015},
