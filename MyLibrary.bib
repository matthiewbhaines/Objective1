	journaltitle = {Food Chemistry},
	author = {Tian, Li and Kejing, Yang and Zhang, Shulin and Yi, Jianhua and Zhu, Zhenbao and Decker, Eric Andrew and {McClements}, David Julian},
	date = {2021-05},
	pmid = {33158675},
	note = {Publisher: Elsevier},
	keywords = {Antioxidant, Emulsions, Lipid oxidation, Prooxidant, Protein oxidation, Tea polyphenols, Whey protein},
}

